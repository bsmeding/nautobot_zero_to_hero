    status__name: "Planned"

  - name: "{{ site_name }}-001"
    "!ref": "rack9"
    location: "!ref:site"
    status__name: "Planned"

{% macro inventory_item(name, part_id, description) -%}
      - name: "{{ name }}"
        manufacturer__name: "Arista"
        part_id: "{{ part_id }}"
        description: "{{ description }}"
{% endmacro %}

