---
locations:
  - name: "{{ site_name }}"
    parent__name: "{{ region.name }}"
    location_type__name: "Site"
    status__name: "Planned"
    "!ref": "site"
    prefixes:
      - prefix: "{{ site_prefix }}"
        status__name: "Active"
        "!ref": "parent_prefix"

# Ensure required device roles exist
roles:
  - "!create_or_update:name": "Core Switch"
    color: "6f42c1"
    description: "Core switching devices"
    content_types:
      - "!get:app_label": "dcim"
        "!get:model": "device"

# Parent Prefix Containers (created first for IPAM hierarchy)
prefixes:
  # Core Router parent containers
  - prefix: "{{ site_prefix | network_offset('1.0.0.0/16') }}"
    status__name: "Active"
    description: "Core Router Interface Networks"
    
  # Spine parent containers  
  - prefix: "{{ site_prefix | network_offset('2.0.0.0/16') }}"
    status__name: "Active"
    description: "Spine Switch Interface Networks"
    
  # Leaf parent containers
  - prefix: "{{ site_prefix | network_offset('3.0.0.0/16') }}"
    status__name: "Active"
    description: "Leaf Switch Interface Networks"

racks:
  - name: "{{ site_name }}-101"
    "!ref": "rack1"
    location: "!ref:site"
    status__name: "Planned"

  - name: "{{ site_name }}-201"
    "!ref": "rack2"
    location: "!ref:site"
    status__name: "Planned"

  - name: "{{ site_name }}-301"
    "!ref": "rack3"
    location: "!ref:site"
    status__name: "Planned"

  - name: "{{ site_name }}-302"
    "!ref": "rack4"
    location: "!ref:site"
    status__name: "Planned"

  - name: "{{ site_name }}-401"
    "!ref": "rack5"
    location: "!ref:site"
    status__name: "Planned"

  - name: "{{ site_name }}-402"
    "!ref": "rack6"
    location: "!ref:site"
    status__name: "Planned"

  - name: "{{ site_name }}-403"
